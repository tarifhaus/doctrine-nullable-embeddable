{
    "name": "tarifhaus/doctrine-nullable-embeddable",
    "description": "A workaround implementation for nullable embeddables in Doctrine entities.",
    "license": "MIT",

    "autoload": {
        "psr-4": { "Tarifhaus\\Doctrine\\": "" },
        "exclude-from-classmap": [ "/Tests/" ]
    },

    "autoload-dev": {
        "psr-4": { "Tarifhaus\\Tests\\Doctrine\\ORM\\": "Tests" }
    },

    "minimum-stability": "stable",
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/property-access": "^3.2 || ^4.0 || ^5.0 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.5 || ^10.0"
    }
}
